@model IEnumerable<obligatorio_PIII.Models.patrocinadores>

@{
    ViewBag.Title = "Nuestros Patrocinadores";
}

<h2 class="mb-4">Nuestros Patrocinadores</h2>

<div class="row">
    @foreach (var p in Model)
    {
        <div class="col-md-4 mb-4">
            <div class="card shadow rounded p-3 h-100">
                <h4 class="card-title">@p.Nombre</h4>
                <p class="card-text">@p.Descripcion</p>
            </div>
        </div>
    }
</div>
